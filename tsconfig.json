{
  "exclude": ["node_modules", "**/*.test.ts", "test/**/*", "example/**/*"],
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "moduleResolution": "node",
    "lib": ["es2017", "dom"],
    "jsx": "react",
    "strict": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "downlevelIteration": true
  }
}
